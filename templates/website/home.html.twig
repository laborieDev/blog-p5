				<div class="row justify-content-center">
					<h1>Mes articles</h1>
					<div class="cats">
						<div class="see-cats-btn main-btn border-btn wow fadeInUp" id="cat-blog-btn" data-wow-delay=".6s" style="visibility: visible; animation-delay: 0.6s; animation-name: fadeInUp;">Catégories <i class="lni lni-list"></i></div>
						<div id="blog-cats">
							{% for cat in cats %}
								<a onclick="setCatPost(this)" categoryid="{{ cat.id }}" class="set_cat_btn" id="set-cat-btn-{{ cat.id }}">{{ cat.name }} <i class="lni lni-chevron-right"></i></a>
							{% endfor %}
						</div>
						<div class="blog-cats mobile">
							<h3 class="main-btn border-btn wow fadeInUp" data-wow-delay=".6s">Catégories <i class="lni lni-list"></i></h3><br>
							{% for cat in cats %}
								<a onclick="setCatPost(this)" categoryid="{{ cat.id }}" class="set_cat_btn" id="set-cat-btn-mobile-{{ cat.id }}">{{ cat.name }} <i class="lni lni-chevron-right"></i></a><br>
							{% endfor %}
						</div>
					</div>
					<div id="blog-list" class="list row wow fadeInLeft" data-wow-delay=".5s">
					{% set i = 1 %}
					{% for post in posts %}
						<div class="single single-{{ i }} col-lg-6" style="background-image:url('{{domain}}/assets/img/single_post/{{ post.img }}')">
							<div class="infos">
								<div>
									<h2>{{ post.title }}</h2>
									<p class="extract">{{ post.extract }}</p>
									<a href="{{ domain }}/article/{{ post.id }}" class="btn view-more">En voir plus</a>
